package tsp;

import java.util.ArrayList;

public class Main {

	public static double distanceBtw(City city1, City city2){
		double xDist = Math.abs(city1.x - city2.x);
		double yDist = Math.abs(city1.y - city2.y);
		double Distance = Math.sqrt((xDist*xDist)+(yDist*yDist));
		return Distance;
	}
	
	public static ArrayList<City> readFile(){
		ArrayList<City> newtour = new ArrayList<City>();
		
		return newtour;
	}
	
	public static void main(String[] args){
		
	}
}
